{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && npm run move-manifest",
        "render": "vite build --config=vite.config.render.js",
        "herd": "vite --config=vite.config.herd.js",
        "docker:dev": "docker-compose up nginx vite -d && echo 'App: https://kayak-map.test (add to /etc/hosts: 127.0.0.1 kayak-map.test)' && docker-compose logs -f vite",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
        "move-manifest": "node move-manifest.js",
        "swagger": "php artisan l5-swagger:generate",
        "setup": "bash devops/setup/project-setup.sh",
        "fresh": "bash devops/setup/fresh-install.sh",
        "fresh:deep": "bash devops/setup/fresh-install.sh --deep",
        "db:backup": "bash devops/database/db-backup-safe.sh",
        "db:restore": "bash devops/database/db-restore.sh",
        "db:test": "bash devops/database/db-test-restore.sh",
        "db:cleanup": "bash devops/database/cleanup-test.sh",
        "macos:check": "bash devops/setup/macos-compatibility.sh",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:build": "docker-compose build vite",
        "docker:logs": "docker-compose logs -f vite"
    },
    "devDependencies": {
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "sass-embedded": "^1.93.3",
        "vite-plugin-browser-sync": "^3.0.4",
        "vite-plugin-vuetify": "^2.1.2"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "@vitejs/plugin-vue": "^5.1.4",
        "@vue-leaflet/vue-leaflet": "^0.10.1",
        "@vueuse/core": "^10.11.1",
        "@vueuse/head": "^2.0.0",
        "@vueuse/integrations": "^14.0.0",
        "axios": "1.12.0",
        "jsonwebtoken": "^9.0.2",
        "jwt-decode": "^4.0.0",
        "laravel-vite-plugin": "^1.0.5",
        "leaflet": "^1.9.4",
        "leaflet-easyprint": "^2.1.9",
        "leaflet.markercluster": "^1.5.3",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.45",
        "railway": "^2.0.17",
        "sortablejs": "^1.15.6",
        "vite": "7.1.11",
        "vue": "^3.4.32",
        "vue-leaflet": "^0.1.0",
        "vue-leaflet-markercluster": "^0.6.1",
        "vue-meta": "^2.4.0",
        "vue-moment": "^4.1.0",
        "vue-router": "^4.0.13",
        "vuedraggable": "^4.1.0",
        "vuetify": "^3.6.13",
        "vuex": "^4.1.0"
    }
