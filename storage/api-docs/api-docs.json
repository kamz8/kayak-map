    "paths": {
        "/trails": {
            "get": {
                "tags": [
                    "Trails"
                ],
                "summary": "Pobierz listę szlaków",
                "operationId": "ccbc89b79250b0f22cbefe7e39a15114",
                "parameters": [
                    {
                        "name": "start_lat",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "end_lat",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "start_lng",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "end_lng",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista szlaków",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TrailCollection"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/trails": {
            "get": {
                "tags": [
                    "Trails"
                ],
                "summary": "Get list of trails",
                "operationId": "a702ce51fb197b6cdbe21e311d3a4bae",
                "parameters": [
                    {
                        "name": "start_lat",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "end_lat",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "start_lng",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "end_lng",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "difficulty",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "łatwy",
                                "umiarkowany",
                                "trudny"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of trails",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TrailCollection"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/trails/{slug}": {
            "get": {
                "tags": [
                    "Trails"
                ],
                "summary": "Get trail details",
                "operationId": "243bf3465191ee636a5191c3079e3106",
                "parameters": [
                    {
                        "name": "slug",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Trail details",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TrailResource"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Trail not found"
                    }
                }
            }
        },
        "/api/v1/nearby-trails": {
            "get": {
                "tags": [
                    "Trails"
                ],
                "summary": "Get nearby trails",
                "operationId": "58066a8884667428b1f16c8e0ccd485a",
                "parameters": [
                    {
                        "name": "lat",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "long",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "number",
                            "format": "float"
                        }
                    },
                    {
                        "name": "location_name",
                        "in": "query",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of nearby trails",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NearbyTrailsCollection"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
